{
  "name": "@odoe/explode",
  "version": "0.0.1",
  "description": "Utility to explode ArcGIS geometries into individual, component geometries.",
  "type": "module",
  "exports": "./lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=14.16"
  },
  "scripts": {
    "prepare": "npm run -s build",
    "build": "npm run format && npm run lint && rimraf lib && npm run build:ts",
    "build:ts": "tsc",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "test": "cypress run"
  },
  "keywords": [
    "esri",
    "arcgis",
    "gis"
  ],
  "author": "Rene Rubalcava <odoe@odoe.net>",
  "license": "MIT",
  "repository": "odoe/explode",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "cypress": "^9.7.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.2"
  },
  "peerDependencies": {
    "@arcgis/core": "^4.23.7"
  }
}
